
public class math21 {

	

	
	public static void main(String[] args) {
		
		//INPUT 
		int a = 100;
		int b = 20;
		int g = 100;
		
        //PROCESS
    	
    	int c = a+b;
    	int d = a-b;
    	int e = a*b;
    	int f = a/b;
    	int h = g-b+a;
    	
    	
    	//OUTPUT
    	System.out.println(a+ "+" +b + " = "  +c);
    	System.out.println(a+ "-" +b + " = " +d);
    	System.out.println(a+ "*" +b + " = "  +e);
    	System.out.println(a+ "/" +b + " = "  +f);
    	System.out.println(g+ "-" +b + "+" + a +   " = "  +h);
		
		

	}

}
